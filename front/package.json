    "@types/node": "^17.0.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "amazon-cognito-identity-js": "^5.2.4",
    "chakra-ui-steps": "^1.5.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "framer-motion": "4",
    "graphql": "^16.2.0",
